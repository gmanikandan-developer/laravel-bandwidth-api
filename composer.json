{
    "name": "palpalani/laravel-bandwidth-api",
    "description": "This is my package LaravelBandwidthApi",
    "keywords": [
        "palPalani",
        "laravel",
        "laravel-bandwidth-api"
    ],
    "homepage": "https://github.com/palpalani/laravel-bandwidth-api",
    "license": "MIT",
    "authors": [
        {
            "name": "palPalani",
            "email": "palani.p@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "bandwidth/iris": "^3.3",
        "bandwidth/sdk": "^8.0",
        "illuminate/contracts": "^8.37|^9.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3|^6.0",
        "orchestra/testbench": "^6.15|^7.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "palPalani\\Bandwidth\\": "src",
            "palPalani\\Bandwidth\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "palPalani\\Bandwidth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "palPalani\\Bandwidth\\BandwidthServiceProvider"
            ],
            "aliases": {
                "Bandwidth": "palPalani\\Bandwidth\\BandwidthFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
